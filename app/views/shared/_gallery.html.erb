<div class="gallery">
	<% if @images.nil? %>
		<p> No Images to Display </p>
	<% else %>
		<% @images.each do |image| %>
			<div class="image">
				<img src="<%= image.url %>"><br>
				<%= image.date %><br>
				<%= image.trail_name %><br>
				<%= image.username %><br>
			</div><br><br>
		<% end %>
	<% end %>
</div>
