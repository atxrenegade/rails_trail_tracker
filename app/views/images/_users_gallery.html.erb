<%= link_to "Add Image", [:new, @imageable, :image] %><br><br>

<h1>My Gallery</h1><br>
<div id="users-gallery">
	<% @images.each do |image| %>
	<div class="image">
		<%= image.url %><br>
		<%= image.date %><br>
		<%= image.trail_name %><br>
		<%= image.username %><br>
	</div><br><br>
	<% end %>
</div>
